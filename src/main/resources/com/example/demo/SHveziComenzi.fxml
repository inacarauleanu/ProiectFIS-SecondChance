<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="497.0" prefWidth="603.0" style="-fx-background-color: #fcc0c7;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.SHVeziComenziController">
   <children>
      <Label layoutX="25.0" layoutY="37.0" prefHeight="31.0" prefWidth="307.0" text="Your Store's Orders">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <TableView fx:id="OrderTable" layoutX="25.0" layoutY="110.0" prefHeight="346.0" prefWidth="376.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" style="-fx-color: #fcc0c7;" text="ID" />
          <TableColumn fx:id="usernameCol" prefWidth="75.0" style="-fx-color: #fcc0c7;" text="Name" />
            <TableColumn fx:id="colorCol" prefWidth="75.0" style="-fx-color: #fcc0c7;" text="Color" />
            <TableColumn fx:id="sizeCol" prefWidth="75.0" style="-fx-color: #fcc0c7;" text="Size" />
            <TableColumn fx:id="priceCol" prefWidth="75.0" style="-fx-color: #fcc0c7;" text="Price" />
        </columns>
      </TableView>
      <Button fx:id="closeButton1" layoutX="575.0" layoutY="-1.0" mnemonicParsing="false" onAction="#closeButton1OnAction" prefHeight="25.0" prefWidth="28.0" style="-fx-background-color: #000000;" text="X" textFill="#fcc0c7" />
      <Button fx:id="goback" layoutX="527.0" layoutY="430.0" mnemonicParsing="false" onAction="#goBackOnAction" style="-fx-background-color: #000000;" text="back" textFill="#fcc0c7" />
   </children>
</AnchorPane>
